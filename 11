#include <iostream>
#include <cstring>
using namespace std;

int main() {
    char str[100], temp[100];
    cout << "Enter a string: ";
    cin.getline(str, 100);

    strcpy(temp, str);
    strrev(temp);

    if(strcmp(str, temp) == 0)
        cout << "String is Palindrome.";
    else
        cout << "String is not Palindrome.";
    return 0;
}
