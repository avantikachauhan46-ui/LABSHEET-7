#include <iostream>
#include <cstring>
using namespace std;

int main() {
    char str1[100], str2[50];
    cout << "Enter main string: ";
    cin.getline(str1, 100);
    cout << "Enter string to search: ";
    cin.getline(str2, 50);

    if(strstr(str1, str2))
        cout << "String is present.";
    else
        cout << "String not found.";
    return 0;
}
